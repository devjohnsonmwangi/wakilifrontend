{
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": ["/static/images/*.{png,jpg,gif}", "/static/css/*"]
    },
    "mimeTypes": {
        ".json": "text/json"
    },
    "redirects": [
        {
            "source": "**",
            "destination": "/",
            "statusCode": 301
        }
    ],
    "routes": [
        {
            "route": "/*",
            "serve": "index.html",
            "statusCode": 200
        }
    ]
}